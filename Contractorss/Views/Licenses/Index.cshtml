@model IEnumerable<Contractorss.License>

@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
    <h2>Licenses</h2>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DateIssue)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Number)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ValidityYear)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateIssue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValidityYear)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LicenseId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LicenseId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LicenseId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
