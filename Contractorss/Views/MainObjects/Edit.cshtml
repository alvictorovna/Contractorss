@model Contractorss.MainObject

@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>Объекты</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MainObjectId" />
            <div class="form-group">
                <label asp-for="NameObject" class="control-label"></label>
                <input asp-for="NameObject" class="form-control" />
                <span asp-validation-for="NameObject" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CustomerId" class="control-label"></label>
                <select asp-for="CustomerId" class="form-control" asp-items="ViewBag.CustomerId"></select>
                <span asp-validation-for="CustomerId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ContractorId" class="control-label"></label>
                <select asp-for="ContractorId" class="form-control" asp-items="ViewBag.ContractorId"></select>
                <span asp-validation-for="ContractorId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ConclusionContract" class="control-label"></label>
                <input asp-for="ConclusionContract" class="form-control" />
                <span asp-validation-for="ConclusionContract" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DeliveryObject" class="control-label"></label>
                <input asp-for="DeliveryObject" class="form-control" />
                <span asp-validation-for="DeliveryObject" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateInputObject" class="control-label"></label>
                <input asp-for="DateInputObject" class="form-control" />
                <span asp-validation-for="DateInputObject" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Назад</a>
</div>

</body>
</html>
